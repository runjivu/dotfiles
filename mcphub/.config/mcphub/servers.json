{
  "nativeMCPServers": [
    
  ],
  "mcpServers": {
    "awslabs.aws-documentation-mcp-server": {
      "disabled": true,
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR",
        "AWS_DOCUMENTATION_PARTITION": "aws"
      },
      "command": "uvx",
      "autoApprove": [
        "read_documentation",
        "search_documentation",
        "recommend"
      ],
      "args": [
        "awslabs.aws-documentation-mcp-server@latest"
      ]
    },
    "kubernetes": {
      "disabled": false,
      "command": "npx",
      "autoApprove": [
        "explain_resource",
        "kubectl_context",
        "kubectl_describe",
        "kubectl_get",
        "kubectl_list",
        "kubectl_logs",
        "list_api_resources"
      ],
      "args": [
        "mcp-server-kubernetes"
      ]
    }
  }
}